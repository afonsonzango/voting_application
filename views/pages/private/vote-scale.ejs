<section class="main-section-select">
    <div class="votes-controllers">
        <div class="middle-efect">
            <div class="bilt-block">
                <div class="upper" id="upperBar1"></div>
                <div class="person">
                    <div class="image">
                        <img src="<%- battle.wariers.warier1[0].img %>" alt="">
                    </div>
    
                    <input type="hidden" value="<%- votes.w1 %>" data-id="<%- battle.wariers.warier1[0].id %>"
                        class="voteSetter" id="warier1" />
                    <div class="name"><%- battle.wariers.warier1[0].nome %></div>
                </div>
            </div>
            <div class="bilt-block">
                <div class="upper" id="upperBar2"></div>
                <div class="person">
                    <div class="image">
                        <img src="<%- battle.wariers.warier2[0].img %>" alt="">
                    </div>
    
                    <input type="hidden" value="<%- votes.w2 %>" data-id="<%- battle.wariers.warier2[0].id %>"
                        class="voteSetter" id="warier2" />
                    <div class="name"><%- battle.wariers.warier2[0].nome %></div>
                </div>
            </div>
        </div>
    
        <div class="endVotation" data-bs-toggle="modal" data-bs-target="#exampleModal">
            <button class="end-btn">
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-x-lg"
                    viewBox="0 0 16 16">
                    <path
                        d="M2.146 2.854a.5.5 0 1 1 .708-.708L8 7.293l5.146-5.147a.5.5 0 0 1 .708.708L8.707 8l5.147 5.146a.5.5 0 0 1-.708.708L8 8.707l-5.146 5.147a.5.5 0 0 1-.708-.708L7.293 8z" />
                </svg>
                Terminar
            </button>
        </div>
    
        <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered">
                <div class="modal-content">
                    <div class="modal-body">
                        <div class="title">Deseja fechar?</div>
                        <div class="text">Lorem ipsum dolor voluptatem qui atque voluptas soluta earum at quisquam tempora, accusantium provident error voluptate, iusto placeat voluptatum inventore?</div>
                        <div class="controlls">
                            <button class="btn btn-primary" data-bs-dismiss="modal">Cancelar</button>
                            <button class="btn btn-danger" data-end-votation>Terminar</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="btn-back-button">
        <a href="/dashboard/arquive-votation">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-left" viewBox="0 0 16 16">
                <path fill-rule="evenodd" d="M15 8a.5.5 0 0 0-.5-.5H2.707l3.147-3.146a.5.5 0 1 0-.708-.708l-4 4a.5.5 0 0 0 0 .708l4 4a.5.5 0 0 0 .708-.708L2.707 8.5H14.5A.5.5 0 0 0 15 8"/>
            </svg>
            Regressar
        </a>
    </div>
    <div class="aftervote" style="display: none;">
        <div class="person" data-person1>
            <img src="<%- battle.wariers.warier1[0].img %>" alt="">
            <div class="values" data-voted1>0</div>
        </div>
